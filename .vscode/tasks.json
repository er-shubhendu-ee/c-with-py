{
    "version": "2.0.0",
    "tasks": [
        {
            "type": "shell",
            "label": "gcc.exe build c capsule",
            "command": "C:/cygwin64/bin/gcc.exe",
            "args": [
                "-g",
                "-I${workspaceFolder}/inc",
                "-I${workspaceFolder}/../lib_redef/c-library/inc",
                "-I${workspaceFolder}/../parsers/argtable3/src",
                "-IC:/cygwin64/usr/include/machine",
				"-IC:/Python38/include",
                "${workspaceFolder}/../lib_redef/c-library/src/stdlib_redef.c",
                "${workspaceFolder}\\src\\c_with_py.c",
                "-o",
                "${workspaceFolder}\\c_capsule.exe"
            ],
            "options": {
                "cwd": "C:\\cygwin64\\bin"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": "build"
        }
    ]
}